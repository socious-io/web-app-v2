    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.4.2",
    "@ionic/pwa-elements": "^3.2.2",
    "@mui/material": "^5.14.14",
    "@mui/x-date-pickers": "^7.5.0",
    "@reduxjs/toolkit": "^2.2.5",
