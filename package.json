    "typescript": "^5.4.3",
    "vite": "^5.2.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint:fix",
      "npm run prettier:fix"
    ],
    "*.{css,scss,md}": "npm run prettier:fix"
  }
